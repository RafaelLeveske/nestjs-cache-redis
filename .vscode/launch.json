{
  "version": "0.1.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug DEV",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "start:dev"],
      "sourceMaps": true,
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${workspaceRoot}",
      "console": "integratedTerminal",
      "protocol": "inspector",
      "env": {
        "NODE_ENV": "local"
      }
    }
  ]
}
